{
    "files.eol": "\n",
    "files.exclude": {
        "**/*.scss.d.ts": true,
        "**/node_modules": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.DS_Store": true,
        "dist": true
    },   
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.enable": true,
    "cSpell.words": [
        "Einar",
        "Ingebrigtsen",
        "linkquality",
        "zigbee"
    ]
}